<h2>Login</h2>

<form class="form login-form">
  <div class="form-group">
    <label for="user-login">user name:</label>
    <input class="form-input" type="text" id="user-login" />
  </div>
  <div class="form-group">
    <label for="password-login">password:</label>
    <input class="form-input" type="password" id="password-login" />
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="submit" id="logout-btn">login</button>
  </div>
</form>

<script src="/public/js/logout.js"></script>
